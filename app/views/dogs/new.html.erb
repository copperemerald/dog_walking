<div class="container">
   <div class="cards" style="width: 27rem;">
  <h2>Create a new Dog Profile</h2>
 <div class="card-product-create">

  <%= simple_form_for(@dog) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :location %>
    <%= f.input :photo, as: :file %>
    <%= f.submit%>
  <% end %>

  </div>
</div>
</div>
